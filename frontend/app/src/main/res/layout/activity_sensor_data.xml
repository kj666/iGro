<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SensorDataActivity">

    <ImageView
        android:id="@+id/logoTopImageView"
        android:layout_width="124dp"
        android:layout_height="52dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="145dp"
        android:layout_marginLeft="145dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="141dp"
        android:layout_marginRight="141dp"
        app:srcCompat="@drawable/igro_logo" />

    <TextView
        android:id="@+id/historicalSensorDataTextView"
        android:layout_width="392dp"
        android:layout_height="42dp"
        android:layout_below="@+id/logoTopImageView"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="11dp"
        android:layout_marginRight="11dp"
        android:layout_marginBottom="8dp"
        android:gravity="center_horizontal|center_vertical"
        android:text="HISTORICAL SENSOR DATA"
        android:textAlignment="center"
        android:textSize="14sp"
        app:fontFamily="sans-serif-medium"
        app:layout_constraintBottom_toTopOf="@+id/logoBottomImageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tempRangeTextView"
        app:layout_constraintVertical_bias="0.349" />

    <TextView
        android:id="@+id/graphTextView"
        android:layout_width="92dp"
        android:layout_height="36dp"
        android:layout_below="@+id/historicalSensorDataTextView"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="7dp"
        android:layout_marginLeft="7dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="309dp"
        android:layout_marginRight="309dp"
        android:layout_marginBottom="567dp"
        android:gravity="center_horizontal|center_vertical"
        android:text="GRAPH"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/cardView10"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lowTempEditText"
        app:layout_constraintVertical_bias="0.933" />

    <android.support.v7.widget.CardView
        android:id="@+id/cardView8"
        android:layout_width="match_parent"
        android:layout_height="191dp"
        android:layout_above="@+id/cardView10"
        android:layout_below="@+id/historicalSensorDataTextView"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="53dp"
        android:layout_marginEnd="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginBottom="91dp"
        app:layout_constraintBottom_toTopOf="@+id/logoBottomImageView"
        app:layout_constraintStart_toStartOf="parent">

        <com.jjoe64.graphview.GraphView
            android:id="@+id/graph"
            android:layout_width="match_parent"
            android:layout_height="155dp" />
    </android.support.v7.widget.CardView>


    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="31dp"
        android:layout_below="@+id/cardView8"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="6dp"
        android:layout_marginLeft="6dp"
        android:layout_marginTop="-44dp"
        android:layout_marginEnd="310dp"
        android:layout_marginRight="310dp"
        android:layout_marginBottom="297dp"
        android:gravity="center_horizontal|center_vertical"
        android:text="TABLE"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/cardView10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView23"
        app:layout_constraintTop_toBottomOf="@+id/highTempEditText"
        app:layout_constraintVertical_bias="0.976" />


    <android.support.v7.widget.CardView
        android:id="@+id/cardView10"
        android:layout_width="408dp"
        android:layout_height="144dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="3dp"
        android:layout_marginRight="3dp"
        android:layout_marginBottom="141dp"
        app:layout_constraintBottom_toTopOf="@+id/logoBottomImageView"
        app:layout_constraintEnd_toEndOf="parent">


        <TableLayout
            android:layout_width="230dp"
            android:layout_height="119dp">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:layout_gravity="left"
                    android:text="Date"
                    android:textSize="20dp"

                    />

                <TextView
                    android:layout_marginLeft="50dp"
                    android:text="째C"
                    android:textSize="20dp" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:layout_gravity="left"
                    android:text="20/02/2019"
                    android:textSize="20dp"

                    />

                <TextView
                    android:layout_marginLeft="50dp"
                    android:text="22째C"
                    android:textSize="20dp" />


            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:layout_gravity="left"
                    android:text="21/02/2019"
                    android:textSize="20dp"

                    />

                <TextView
                    android:layout_marginLeft="50dp"
                    android:text="19째C"
                    android:textSize="20dp" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:layout_gravity="left"
                    android:text="22/02/2019"
                    android:textSize="20dp"

                    />

                <TextView
                    android:layout_marginLeft="50dp"
                    android:text="23째C"
                    android:textSize="20dp" />
            </TableRow>


        </TableLayout>
    </android.support.v7.widget.CardView>


</RelativeLayout>